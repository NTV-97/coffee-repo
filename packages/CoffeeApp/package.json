{
  "name": "coffeeapp",
  "version": "0.0.1",
  "private": true,
  "main": "./src/assets/index.ts",
  "types": "./types/assets/index.d.ts",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:ts": "tsc -p ./tsconfig.json",
    "pod": "cd ios && pod install",
    "gen:images": "node scripts/genImages",
    "gen:fonts": "node scripts/genFonts",
    "ios-bundle": "npx react-native bundle --entry-file ../../index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-deploygate": "cd ios && bundle exec fastlane release",
    "zip:adhoc": "cd ios/fastlane/build && zip -r ipa.zip adhoc",
    "archive": "node scripts/archive",
    "open:android-studio": "open -a Android\\ Studio \"android\""
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-navigation/drawer": "^6.4.3",
