    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "apollo-client-preset": "^1.0.8",
    "config": "0.0.1",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-hook": "^0.0.1",
    "graphql-tag": "^2.12.6",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-apollo": "^3.1.5",
    "react-native": "0.71.4",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-iphone-x-helper": "^1.3.1",
